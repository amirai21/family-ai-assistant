{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI: Run Server",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--reload",
        "--port",
        "8000"
      ],
      "jinja": true,
      "cwd": "${workspaceFolder}",
      "env": {
        "ENVIRONMENT": "development"
      },
      "console": "integratedTerminal"
    }
  ]
}
